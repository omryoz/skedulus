/* CSS Document */
 .grid { font-size:10px; }
        .grid .date { width:8em; }
        .h { border-right: 1px solid #999999;}
        .st1 { background-color:#666666;}
        .st2 { background-color:red;}
        .st3 { background-color:blue;}

/* schedule */
    .schedule {position: relative; background-color: #f3f3f3;}
    .schedule span {color: #333333;}
    .schedule > * {float: left; }

        #overlay { }
        #filterForm { width:100%; }
        #filterForm input {min-width: 0; width: 7em; padding:.35em 0.25em; font-size: 1.35em; }
        #filterForm #appointmentServiceSelect_combo {width: 12em;}
        #filterForm #appointmentDate { width:8.5em; border: 1px solid #ccc; }
        .filter {float: left}
        .filter > li {float: left; margin-left: 1em; margin-bottom: .5em;}
        .filter > li:last-child {clear: both; font-weight: bold; margin: 1em;}
        .days { float: left; text-transform: lowercase; margin: 0em 1em 0em 1em; clear:both; width:8.5em; }

        .daysOL { float: left; clear:both; margin-top: 6px; }
        .daysOL li { padding: 0; margin: 0; height: 33px; text-align:right; }
        .daysOL li span { font-size:1.4em;  color:#333; }
        .daysOL li a {color: #a9a9a9;}

        .scroll {float: left; clear:both; display: block;
            width: 8em; height: 1em;  padding: 1px;
            border: solid 1px #cccccc; text-align: center;
            background-color: #fcfcfc;}

        .scroll .sprite {margin-left: 3.5em; }
        .scroll.top { margin-bottom: 1em;  }

    .schedule .days .scroll:hover {background-color: #d9d9d9;}
    .grid { background-color: #f3f3f3; border:1px solid #ccc; padding: 6px;border-right: 0;}
    .grid  > ul { clear: both; }
    .grid li{ border-right: 1px solid #F3F3F3; color:#333;}
    .t00, .t30 { margin-right: 0 !important; border-right: 1px dashed #dddddd !important;}
    .t00.available, .t30.available { margin-right: 0 !important; border-right: 1px dotted #99c4e5 !important;}
   .grid li { width:10.0px;}
   .grid li { text-align: left;}

    .timeBlock {display: block; width: 1.5em; height: 1.5em;}
    .grid td { background-color: #d9d9d9; }
    li.available, .timeBlock.available {background-color: lightgray; }
    li.available.mustCallToBook,  .timeBlock.mustCallToBook {background-color: #90e0F0;}
    li.available.mustCallToBook.selected {background-color: #90e0F0;}
   li.downtime_selected {background-color: #23DAD1;}
    li.selected {background:rgba(111,111,111,0.6)}
    li.booked {background-color: #D9D9D9;}
   li.past {background-color: #b9b9b9;}

	.grid ul li { height: 30px; vertical-align: top; display:inline-block; }
    .grid ul { padding: 0; margin: 0; }
       ul.data { height: 22px; padding: 0; margin: 0; }
	    ul.data li { height: 22px; padding: 0; margin: 0; margin-left: -3px;}
        ul.padding { height: 11px; padding: 0; margin: 0; }
        ul.padding li { height: 11px; background-color: #f3f3f3;
            padding: 0; margin: 0; }
